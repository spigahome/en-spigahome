pigahome.it\/prodotto\/tagliatelle-low-carb\/\">Tagliatelle Low Carb<\/a><\/strong>, da i <a href=\"https:\/\/spigahome.it\/prodotto\/preparato-torta-al-cioccolato-low-carb\/\"><strong>Preparati per Torte Low Carb<\/strong><\/a> e tanto altro.<\/p>","align":"left","_padding":{"unit":"%","top":"0","right":"10","bottom":"0","left":"0","isLinked":false},"cafe_custom_queries_items":[]},"elements":[],"isInner":false,"widgetType":"text-editor","elType":"widget"},{"id":"26c89679","settings":{"button_label":"SCOPRI I NOSTRI PRODOTTI","button_style":"flat","text_align":"center","button_color":"#FFFFFF","button_bg":"#3C3C3B","border_radius":{"unit":"px","top":"0","right":"0","bottom":"0","left":"0","isLinked":true},"dimensions":{"unit":"px","top":"20","right":"50","bottom":"20","left":"50","isLinked":false},"button_color_hover":"#FFFFFF","button_bg_hover":"#000000","_margin_tablet":{"unit":"px","top":"0","right":"0","bottom":"0","left":"0","isLinked":false},"_margin_mobile":{"unit":"px","top":"0","right":"0","bottom":"0","left":"0","isLinked":false},"animation_duration":"slow","link":{"url":"https:\/\/spigahome.it\/prodotti\/","is_external":"","nofollow":"","custom_attributes":""},"cafe_custom_queries_items":[]},"elements":[],"isInner":false,"widgetType":"clever-button","elType":"widget"}],"isInner":false,"elType":"column"},{"id":"4feb2f77","settings":{"_column_size":50,"_inline_size":35.045000000000002,"cafe_custom_queries_items":[]},"elements":[{"id":"65f0832f","settings":{"image":{"url":"https:\/\/spigahome.it\/wp-content\/uploads\/2020\/05\/Spiga-webstyle-chisiamo.png","id":1844},"image_size":"custom","image_custom_dimension":{"width":"","height":"600"},"align":"center","_margin":{"unit":"px","top":"0","right":"0","bottom":"0","left":"0","isLinked":false},"cafe_custom_queries_items":[]},"elements":[],"isInner":false,"widgetType":"image","elType":"widget"}],"isInner":false,"elType":"column"}],"isInner":false,"elType":"section"},{"id":"285d6dfd","settings":{"cafe_custom_que